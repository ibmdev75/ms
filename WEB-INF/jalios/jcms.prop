# TAGS
# ----------------------------------------------------------------------
# Tags Rating
tag.rating.count: 6

tag.highlight.prefix: <b><span class='relevant'>
tag.highlight.suffix: </span></b>

# Thumbnail Tag
# Interval between recheck of failed attempt to create a thumbnail (21600 == 6 hours))
tag.thumbnail.failed-attempt-check-delay: 21600
# Maximum image size (when loaded in memory) above which no thumbnail
# will be generated (in bytes). The comparison is performed with the
# result of the following formula applied to the image : 3 x width x height
# The default value is set to 28 MBytes which should be enough for 10 megapixel images
# Width, Height, Megapixel (pixel), bytes occupied in memory (approximation)  
#   800     600        ( 480000)     1440000
#  1024     768        ( 786432)     2359296
#  1280     960      1 (1228800)     3686400
#  1536    1180        (1812480)     5437440
#  1600    1200      2 (1920000)     5760000
#  2048    1536      3 (3145728)     9437184
#  2240    1680      4 (3763200)    11289600
#  2560    1920      5 (4915200)    14745600
#  3032    2008      6 (6088256)    18264768
#  3072    2304      7 (7077888)    21233664
#  3264    2448      8 (7990272)    23970816
#  3648    2736     10 (9980928)    29942784
tag.thumbnail.max-image-memsize: 30000000
# Thumbshot (thumbnail creator for URLs), leave commented to disable this feature
# You can for example use the following site to generated thumbshots
# tag.thumbnail.thumbshot.url: http://a.searchpreview.de/preview?s=
tag.thumbnail.thumbshot.url: 
tag.thumbnail.thumbshot.file-ext: .jpg

