# ----------------------------------------------------------------------
# Descriptive URLs
# ----------------------------------------------------------------------

descriptive-urls.enabled:         true
descriptive-urls.prefix:          jcms
descriptive-urls.language.enabled:true
descriptive-urls.text.enabled:    true

# Regular expressions used for the text part cleaning process
# 1. Replace spaces and punctation and other special char with dash
descriptive-urls.text.clean.1-regex: [\\s\\p{Punct}\\p{Pc}\\p{Pd}\\p{Pe}\\p{Pf}\\p{Pi}\\p{Po}\\p{Ps}\\p{Sc}\\p{Sk}\\p{Sm}&&[^/]]
descriptive-urls.text.clean.1-replace: -
# 2. Remove unwanted char
descriptive-urls.text.clean.2-regex: [^0-9a-zA-Z/-]
descriptive-urls.text.clean.2-replace: 
# 3. Remove doubled letters '//' '--'
descriptive-urls.text.clean.3-regex: ([/-])\\1+
descriptive-urls.text.clean.3-replace: $1
# 4. Limit length to 1024 characters (improvement created for JCMS-4143)
descriptive-urls.text.clean.4-regex: ^(.{0,1024}).*$
descriptive-urls.text.clean.4-replace: $1


# coma separated ids of root categories used to retrieve first
# publication's category displayed in URLs
# leave empty to use default jcms root cat
descriptive-urls.text.root-cats:  

# Publications descriptive's URLs format
# {0} ==> language
# {1} ==> publication title, display title for portletskinnable
# {2} ==> cdate
# {3} ==> mdate, cdate if null
# {4} ==> pdate, cdate if null
# {5} ==> sdate, cdate if null
# {6} ==> first category name
# 
#descriptive-urls.text.pub-format: {0}/{6}/{4,date,yyyy/MM/dd}/{1}
descriptive-urls.text.pub-format: {1}

# Categories descriptive's URLs format
# {0} ==> language
# {1} ==> category name
# {2} ==> cdate
# {3} ==> mdate, cdate if null
# {4} ==> category ancestor string with '/'
# 
descriptive-urls.text.cat-format: {1}

# Members descriptive's URLs format
# {0} ==> language
# {1} ==> full name
# {2} ==> cdate
# {3} ==> mdate, cdate if null
# {4} ==> first name
# {5} ==> last name
# {6} ==> email
# 
descriptive-urls.text.mbr-format: {1}

# Default Data descriptive's URLs format
# {0} ==> language
# {1} ==> data name
# {2} ==> cdate
# {3} ==> mdate, cdate if null
descriptive-urls.text.dft-format: {1}

