# ----------------------------------------------------------------------
# Display and Query template for core types
# ----------------------------------------------------------------------

# FileDocument
display.FileDocument.default: types/FileDocument/doFileDocumentFullDisplay.jsp
query.FileDocument.default: types/FileDocument/doFileDocumentResultDisplay.jsp

# ----------------------------------------------------------------------
# FileDocuments
# ----------------------------------------------------------------------

# Convenient rexexp used while listing files
file.regexp.ignore: (.*[/\\\\](\\.svn|CVS|Thumbs.db|\\.cvsignore)([/\\\\].*)?)|(.*(~|\\.bak))$

# settings
file-document.urid: false
file-document.remove-file: true
file-document.allow-unzip: true
file-document.default-detail-view: true 
file-document.filename-limit: 128
file-document.show-options.default: false
#file-document.show-options.FileDocument: false
#file-document.show-options.DBFileDocument: false

# Associated Files patterns (where "{filename}" is the FileDocument complete file name).
# WARNING! do not change this without wise knowledge of consequences :
# All files matching this patterns will be DELETED when a filedocument is updated or deleted
file-document.associated-files.thumbnails: ^th-\\d+x\\d+-{filename}\\.jpg$
file-document.associated-files.derived-files: ^{filename}(.\\w+)+$

# List of content-type and generic content-type for which PDF upload must not be proposed
file-document.associated-pdf-upload-blacklist: video image audio

# Plupload I18N mapping : 

# Map JCMS language key to the language key used in Plupload
#  plupload.languagekey-mapping.{jcms-language-key}: {plupload-language-key}
# See UploadManager.java
# plupload.languagekey-mapping.pt: pt_BR
# plupload.languagekey-mapping.zh: zh_CN
# plupload.languagekey-mapping.zh-Hant: zh_TW


# Plupload filters (as used in jalios-plupload.js)
js.plupload-filters: { \
  'images' : { extensions : "jpg,gif,png,jpeg,tif,tiff,svg" }, \
  'videos' : { extensions : "avi,mp4,mpeg,mpg,mov,flv,swf,wmv,ogv,webm" }, \
  'sounds' : { extensions : "mp3,wav,aac,m4a,ogg,oga" }, \
  'zip'    : { extensions : "zip" }, \
  'media'  : { extensions : "jpg,gif,png,jpeg,tif,tiff,svg,avi,mp4,mpeg,mpg,mov,flv,swf,wmv,ogv,webm,mp3,wav,aac,m4a,ogg,oga,zip" }\
}

