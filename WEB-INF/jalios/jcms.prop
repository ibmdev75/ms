# --- JS packer ----------------------
channel.js-packer.enabled: true
# Compressor with default settings : from 20% to 50% decrease in file size
channel.js-packer.compressor.enabled: true
# line-break
#  The linebreak option is used to split long lines after a specific column.
#  It can be used to make the code more readable, easier to debug
#  (especially with the MS Script Debugger).
#  Specify 0 to get a line break after each semi-colon in JavaScript, -1 to disable.
channel.js-packer.compressor.line-break: -1
# Munge : roughly 10% decrease in file size if enabled
#  true ==> Minify and obfuscate local symbols
#  false ==> Minify only, do not obfuscate local symbols.
channel.js-packer.compressor.munge: true
# preserve-semi : no visible change in file size
#  true ==> Preserve unnecessary semicolons (such as right before a '}')
#  false ==> Remove all unnecessary semicolons (such as right before a '}')
channel.js-packer.compressor.preserve-semi: false
# disable-optimization : no visible change in file size
#  true ==> Disable all the built-in micro optimizations.
#  false ==> Enable all the built-in micro optimizations.
channel.js-packer.compressor.disable-optimizations: false
# verbose
#  true ==> Log informational messages and warnings
#  false ==> Nothing logged
channel.js-packer.compressor.verbose: false
# -------------------------------------

# --- CSS packer ----------------------
channel.css-packer.enabled: true
channel.css-packer.compressor.enabled: true
# The linebreak option is used to split long lines after a specific column.
# It can be used to make the code more readable.
# Specify 0 to get a line break after each rule in CSS, -1 to disable.
channel.css-packer.compressor.line-break: -1
