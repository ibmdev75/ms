# Rewrite object>embed tag in HTML wysiwyg content
# with content generated by the MediaTag and its template API.  
# This process occurs at rendering time, through WysiwygRenderer API 
# invoked by <jalios:wysiwyg> tag.
# cf http://issues.jalios.com/browse/JCMS-3680
wysiwyg.medias-rewrite.enabled: true

# WIKIwyg --------------------------------------------------------------

# Textarea CSS selector
wikiwyg.tinymce.editor-selector: formWikiwyg

# Path of the CSS file to be used by tinymce during edition
# wikiwyg.tinymce.content-css: css/tinymce-wikiwyg.css
  wikiwyg.tinymce.content-css: css/jalios/core/core.css

# plugins
wikiwyg.tinymce.plugins: autolink,spellchecker,preview,searchreplace,nonbreaking,jcms,fullscreen

# toolbars
wikiwyg.tinymce.toolbar-1: bold,italic,|,hr,|,formatselect,|,bullist,numlist,outdent,indent,|,jcmsPublink,link,unlink,|,jcmsMedia,media,|,search,replace,spellchecker,|,fullscreen,jcmsWikiPreview,jcmsWikiCode,
wikiwyg.tinymce.toolbar-2: 
wikiwyg.tinymce.toolbar-3: 
wikiwyg.tinymce.toolbar-4: 
wikiwyg.tinymce.toolbar-5: 

# default JCMS options 
# WARNING: should not be changed without Jalios advice
# (do NOT start NOR end with a coma)
wikiwyg.tinymce.jcms-init-options:\
    theme : 'advanced',  \n\t\t\
    entity_encoding : 'raw', // needed as we want data to be store in UTF-8  \n\t\t\
    apply_source_formatting : false, // if set to true, line breaks would appear after wiki2html conversion  \n\t\t\
    add_unload_trigger : true,  \n\t\t\
    valid_elements : "p,strong/b,em/i,code,hr,br,h1,h2,h3,img[src|width|height|alt],a[href],ul,ol,li,dd,dl,dt,pre,blockquote", \n\t\t\
    theme_advanced_blockformats : 'p,h1,h2,h3,pre,blockquote,dt,dd,code', \n\t\t\
    theme_advanced_toolbar_location : 'top',  \n\t\t\
    theme_advanced_toolbar_align : 'left',   \n\t\t\
    theme_advanced_statusbar_location : 'bottom', \n\t\t\
    theme_advanced_resizing : true, \n\t\t\
    theme_advanced_resizing_use_cookie : false, \n\t\t\
    preformatted : true, \n\t\t\
    jcms_wikiwyg : true \n\t\t\

